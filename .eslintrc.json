	"extends": ["@higoralves/eslint-config/react/ts/native"],
	"rules": {
		"import/prefer-default-export": "off",
		"no-use-before-define":"off",
		"import-helpers/order-imports": [
			"warn",
			{
				"newlinesBetween": "always",
				"groups": ["/react/", "/Ë†react/", "module", "/^@/Config/", "/^@/Lib/","/^@/Services/","/^@/Store/","/^@/Hooks/","/^@/Routers/","/^@/Containers/","/^@/Components/","/^@/View/", ["parent", "sibling", "index"]],
